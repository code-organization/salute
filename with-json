            <script>

                var $j = jQuery.noConflict();
                console.log("Before function");
                $j(document).ready(function($) {


                    $.ajaxSetup({cache: false});
                    $.getJSON("https://www.nba.com/.element/media/2.0/teamsites/spurs/Rosalie/salute-data.json",

                        function makeBannerInfo(data) {
                            console.log("after Json call");

                            var gameInfo = data.gscd.g;

                            gameInfo.forEach(function (content, i) {


                                var gameDate = content.gdte;
                                var dateFomatted = gameDate.substring(5, 7) + "/" + gameDate.substring(8, 10);
                                var gameTime = content.htm;
                                var gameTime2 = content.htm;
                                var gameTimeFormatted = gameTime.substring(11, 13);
                                var gameTimeMinutesString = gameTime2.substring(14, 16);
                                var gameTimeNumber = parseInt(gameTimeFormatted - 12);
                                var opponentInfo = content.v;
                                var opponentName = content.v.tn;
                                var opponentCity = content.v.tc;

                                var urlLogo = content.v.ta;
                                var homeGame = content.an;
                                var awayGame = content.h.ta;
                                var markup = '';
                                var dayOfWeek = new Date(gameTime2);
                                var day1 = dayOfWeek.getDay();

                                var today = new Date();
                                var mm = today.getMonth() + 1;
                                var dd = today.getDate();
                                var yyyy = today.getFullYear();
                                var date = new Date(yyyy, mm, dd);

                                var gameScheduleDate = new Date(gameTime);
                                var weekday = new Array(7);
                                weekday[0] = "SUNDAY";
                                weekday[1] = "MONDAY";
                                weekday [2] = "TUESDAY";
                                weekday [3] = "WEDNESDAY";
                                weekday [4] = "THURSDAY";
                                weekday [5] = "FRIDAY";
                                weekday [6] = "SATURDAY";

                                var inputDate = weekday[day1];
                                var opponentInHomeCity = content.h.tc;
                                var opponentHomeTeam = content.h.tn;
                                var opponentFullAtHome = opponentInHomeCity + " " + opponentHomeTeam;
                                var opponentAtHomeLogo = content.h.ta;
                                var awayStadium = content.an;
                                var timeAway = content.vtm;
                                var awayTimeFormatted = timeAway.substring(11, 13);
                                var awayTimeNumber = parseInt(awayTimeFormatted - 12);

                                var broadcastHome = content.bd.b[0].scope;
                                var homeChannel = content.bd.b[0].disp;
                                var homeChannel2 = content.bd.b[1].disp;

                                var awayChannel1 = content.bd.b[0].disp;
                                var awayChannel2 = content.bd.b[1].disp;
                                //var awayChannel3 = content.bd.b[2].disp;
                                var broadcastAway = content.bd.b[0].scope;
                                var infoFromTicketJson = content.link;


                                if (today < gameScheduleDate) {
                                    if (homeGame == "AT&T Center") {


                                        markup += "<li class='all-orientations all-months all-days all-opponents'>";
                                        markup += "<div class='games clearfix'>";
                                        markup += "<div class='col-xs-5 pull-left text-center'>";
                                        markup += "<div class='row'>";
                                        markup += "<div class='col-sm-4 bigBold text-center borderRight'>";
                                        markup += dateFomatted;

                                        markup += "</div>";
                                        markup += "<div class='col-sm-8 borderRight'>";
                                        markup += "<div class='row spurstitles' style='text-transform:uppercase'>";
                                        markup += inputDate + "<br>" + gameTimeNumber + ':' + gameTimeMinutesString + "PM";
                                        markup += "</div>";
                                        markup += "</div>";
                                        markup += '</div>';
                                        markup += "</div>";
                                        markup += "<div class='col-xs-5  pull-left text-center borderRight'>";
                                        markup += "<div class='col-sm-6 col-md-4 col-lg-6 noPadding'>";
                                        markup += '<img class="logoImage" src="https://www.nba.com/.element/media/2.0/teamsites/spurs/Rosalie/img/nba-logos/' +urlLogo +'.gif" width="60px" height="60px">';

                                        markup += "</div>";
                                        markup += "<div class='col-sm-6 col-md-4 col-lg-6'>";
                                        markup += "<div class='row spurstitles'>";
                                        markup += opponentCity + " " + opponentName;
                                        markup += "</div>";
                                        markup += "</div>";

                                        markup += "</div>";

                                        markup += "<div class='col-md-2 col-xs-12 pull-left text-center'>";
                                        markup += "<div class='buyButton'>";
                                        markup += "<a class='btn btn-warning btn-sm blackBtn' target='_blank' href=" + '"' + infoFromTicketJson + '"' + ">";
                                        markup += "Tickets";
                                        markup += "</a>";
                                        markup += "</div>";
                                        markup += "</div>";
                                        markup += "</div>";
                                        markup += "</li>";
                                        $("#inputContent").append(markup)




                                    }
                                }
                            });

                        });

                });

            </script>


            <script>

                // Function for Rampage schedule

                var $j= jQuery.noConflict();
                console.log("Rosalie");
                $j(document).ready(function($) {

                    console.log("before JSON");
                    $.ajaxSetup({cache:false});
                    $.getJSON("https://www.nba.com/.element/media/2.0/teamsites/spurs/Rosalie/rampage.json",

                        function loadRampageInfo(data){
                            console.log("11111111");
                            var rampageInformation = data;
                            var scheduleInfo = data.SiteKit.Schedule;

                            scheduleInfo.forEach(function(content, i){
                                var gameDateEach = content.date_time_played;
                                var today = new Date();
                                var homegame = content.home_team_city;
                                var formattedDate = gameDateEach.substring(11,13);
                                var gameHour = content.scheduled_time.substring(0,7);
                                var gameScheduleDate1 = new Date(gameDateEach);
                                var dateDays = gameDateEach.substring(8,10);
                                console.log(dateDays);
                                var minuteSting = gameDateEach.substring(14,16);
                                var opponentInfo = content.visiting_team_name;
                                var opponentCode = content.visiting_team_code;
                                var ticketlink = content.tickets_url;

                                var dayOfWeek1 = new Date(gameDateEach);
                                var day1 = dayOfWeek1.getDay();

                                var weekday = new Array(7);
                                weekday[0] = "SUNDAY";
                                weekday[1] = "MONDAY";
                                weekday[2] = "TUESDAY";
                                weekday[3] = "WEDNESDAY";
                                weekday[4] = "THURSDAY";
                                weekday[5] = "FRIDAY";
                                weekday[6] = "SATURDAY";

                                var inputDate1 = weekday[day1];

                                var monthName = new Array(12);
                                monthName [0] = "January";
                                monthName [1] = "February";
                                monthName[2] = "March";
                                monthName[3] = "April";
                                monthName[4] = "May";
                                monthName[5] = "June";
                                monthName[6] = "July";
                                monthName[7] = "August";
                                monthName[8] = "September";
                                monthName[9] = "October";
                                monthName[10] = "November";
                                monthName[11] = "December";

                                var inputMonth = monthName[day1];
                                var correctMonth = monthName[dayOfWeek1.getMonth()];






                                var markup = " ";

                                if(today < gameScheduleDate1){
                                    if(homegame == "San Antonio") {

                                        console.log(inputMonth);

                                        markup +="<div class='row gamerow home'>";
                                        markup +="<div class='col-sm-3 col-xs-4 day'>";
                                        markup += inputDate1+ ", " + correctMonth + " " + dateDays+ '<br>' + gameHour;
                                        markup += "</div>";
                                        markup += "<div class='col-sm-3 col-xs-4 logoimg'>";
                                        markup += '<img src="http://www.nba.com/.element/media/2.0/teamsites/spurs/Rosalie/img/rampage/' + opponentCode + '.png" >';

                                        markup += "</div>";
                                        markup += "<div class='col-sm-3 opponent'>";
                                        markup += opponentInfo;
                                        markup += "</div>";
                                        markup += "<div class='col-sm-3 col-xs-4'><a href="+ ticketlink + " target='_blank' class='btn btn-success'>Tickets</a>";
                                        markup += "</div>";

                                        markup += "</div>";

                                        $("#rampageInput").append(markup);

                                    }
                                }

                            });




                        });
                });


            </script>
